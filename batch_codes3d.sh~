#!/bin/bash

#snp_file="/mnt/3dgenome/projects/tfad334/hvn/diabetes/results/codes3d_results/snps/undone_snps.txt"
#wc -l $snp_file
snpdir="../data/batch_snps/done"
#split -l 50 -d $snp_file
#mv $snp_file ../
mkdir ../data/batch_results1
mkdir ../data/batch_snps/done1
results_dir="../data/batch_results1"
done_dir="../data/batch_snps/done1"
for f in $snpdir/*
do
    echo Processing $f 
    python ../../codes3d/codes3d/codes3d.py -i $f -o $results_dir/$f -r mboi -c /mnt/3dgenome/projects/tfad334/codes3d/docs/codes3d.conf
    mv $f done_dir
done
